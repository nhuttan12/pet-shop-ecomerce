{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2020",
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "baseUrl": "./src",
    "paths": {
      "@auth": [
        "core/auth"
      ],
      "@auth/*": [
        "core/auth/*"
      ],
      "@cart": [
        "core/cart"
      ],
      "@cart/*": [
        "core/cart/*"
      ],
      "@contact": [
        "core/contact"
      ],
      "@contact/*": [
        "core/contact/*"
      ],
      "@order": [
        "core/order"
      ],
      "@order/*": [
        "core/order/*"
      ],
      "@product": [
        "core/product"
      ],
      "@product/*": [
        "core/product/*"
      ],
      "@user": [
        "core/user"
      ],
      "@user/*": [
        "core/user/*"
      ],
      "@role": [
        "core/role"
      ],
      "@role/*": [
        "core/role/*"
      ],
      "@category": [
        "core/category"
      ],
      "@category/*": [
        "core/category/*"
      ],
      "@brand": [
        "core/brand"
      ],
      "@brand/*": [
        "core/brand/*"
      ],
      "@wishlist": [
        "core/wishlist"
      ],
      "@wishlist/*": [
        "core/wishlist/*"
      ],
      "@voucher": [
        "core/voucher"
      ],
      "@voucher/*": [
        "core/voucher/*"
      ],
      "@comment": [
        "core/comment"
      ],
      "@comment/*": [
        "core/comment/*"
      ],
      "@post": [
        "core/post"
      ],
      "@post/*": [
        "core/post/*"
      ],
      "@payment": [
        "core/payment"
      ],
      "@payment/*": [
        "core/payment/*"
      ],
      "@api-response/*": [
        "common/api-response/*"
      ],
      "@class-validator/*": [
        "common/class-validator-contraint/*"
      ],
      "@config/*": [
        "common/config/*"
      ],
      "@constants/*": [
        "common/constants/*"
      ],
      "@db/*": [
        "common/database/*"
      ],
      "@decorators/*": [
        "common/decorator/*"
      ],
      "@filters/*": [
        "common/filter/*"
      ],
      "@guards/*": [
        "common/guard/*"
      ],
      "@images/*": [
        "common/image/*"
      ],
      "@mail/*": [
        "common/mail/*"
      ],
      "@messages/*": [
        "common/messages/*"
      ],
      "@pagination/*": [
        "common/pagination/*"
      ],
      "@paypal/*": [
        "common/paypal/*"
      ],
      "@services/*": [
        "common/services/*"
      ]
    }
  }
}
