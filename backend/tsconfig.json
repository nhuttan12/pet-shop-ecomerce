{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2020",
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "baseUrl": "./src",
    "paths": {
      "@auth": [
        "auth"
      ],
      "@auth/*": [
        "auth/*"
      ],
      "@cart": [
        "cart"
      ],
      "@cart/*": [
        "cart/*"
      ],
      "@contact": [
        "contact"
      ],
      "@contact/*": [
        "contact/*"
      ],
      "@order": [
        "order"
      ],
      "@order/*": [
        "order/*"
      ],
      "@product": [
        "product"
      ],
      "@product/*": [
        "product/*"
      ],
      "@user": [
        "user"
      ],
      "@user/*": [
        "user/*"
      ],
      "@role": [
        "role"
      ],
      "@role/*": [
        "role/*"
      ],
      "@category": [
        "category"
      ],
      "@category/*": [
        "category/*"
      ],
      "@brand": [
        "brand"
      ],
      "@brand/*": [
        "brand/*"
      ],
      "@wishlist": [
        "wishlist"
      ],
      "@wishlist/*": [
        "wishlist/*"
      ],
      "@voucher": [
        "voucher"
      ],
      "@voucher/*": [
        "voucher/*"
      ],
      "@comment": [
        "comment"
      ],
      "@comment/*": [
        "comment/*"
      ],
      "@post": [
        "post"
      ],
      "@post/*": [
        "post/*"
      ],
      "@api-response/*": [
        "common/api-response/*"
      ],
      "@class-validator/*": [
        "common/class-validator-contraint/*"
      ],
      "@config/*": [
        "common/config/*"
      ],
      "@constants/*": [
        "common/constants/*"
      ],
      "@db/*": [
        "common/database/*"
      ],
      "@decorators/*": [
        "common/decorator/*"
      ],
      "@filters/*": [
        "common/filter/*"
      ],
      "@guards/*": [
        "common/guard/*"
      ],
      "@images/*": [
        "common/image/*"
      ],
      "@mail/*": [
        "common/mail/*"
      ],
      "@messages/*": [
        "common/messages/*"
      ],
      "@pagination/*": [
        "common/pagination/*"
      ],
      "@paypal/*": [
        "common/paypal/*"
      ],
      "@services/*": [
        "common/services/*"
      ]
    }
  }
}
